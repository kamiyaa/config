{
    "keymap": [
        {
            "keys": [ "q" ],
            "command": "close"
        },
        {
            "keys": [ "r" ],
            "command": "reload_dirlist"
        },
        {
            "keys": [ "z", "h" ],
            "command": "toggle_hidden"
        },
        {
            "keys": [ "arrow_up" ],
            "command": "cursor_move_up"
        },
        {
            "keys": [ "arrow_down" ],
            "command": "cursor_move_down"
        },
        {
            "keys": [ "arrow_left" ],
            "command": "cd .."
        },
        {
            "keys": [ "arrow_right" ],
            "command": "open"
        },
        {
            "keys": [ "\n" ],
            "command": "open"
        },
        {
            "keys": [ "end" ],
            "command": "cursor_move_end"
        },
        {
            "keys": [ "home" ],
            "command": "cursor_move_home"
        },
        {
            "keys": [ "page_up" ],
            "command": "cursor_move_page_up"
        },
        {
            "keys": [ "page_down" ],
            "command": "cursor_move_page_down"
        },
        {
            "keys": [ "\t" ],
            "command": "toggle_view"
        },
        {
            "keys": [ "c", "d" ],
            "command": ":cd "
        },
        {
            "keys": [ "t" ],
            "command": "select --all=true --toggle=true"
        },
        {
            "keys": [ ":" ],
            "command": ":"
        },
        {
            "keys": [ ";" ],
            "command": ":"
        },
        {
            "keys": [ "?" ],
            "command": ":search "
        },
        {
            "keys": [ "\\" ],
            "command": ":search_glob "
        },
        {
            "keys": [ "/" ],
            "command": "search_skim"
        },
        {
            "keys": [ "[" ],
            "command": "search_prev"
        },
        {
            "keys": [ "]" ],
            "command": "search_next"
        },
        {
            "keys": [ "u", "r" ],
            "command": "sort reverse"
        },
        {
            "keys": [ "u", "l" ],
            "command": "sort lexical"
        },
        {
            "keys": [ "u", "m" ],
            "command": "sort mtime"
        },
        {
            "keys": [ "u", "n" ],
            "command": "sort natural"
        },
        {
            "keys": [ "u", "s" ],
            "command": "sort size"
        },
        {
            "keys": [ "u", "e" ],
            "command": "sort ext"
        },
        {
            "keys": [ "g", "r" ],
            "command": "cd /"
        },
        {
            "keys": [ "g", "c" ],
            "command": "cd ~/.config"
        },
        {
            "keys": [ "g", "d" ],
            "command": "cd ~/Downloads"
        },
        {
            "keys": [ "g", "e" ],
            "command": "cd /etc"
        },
        {
            "keys": [ "g", "h" ],
            "command": "cd ~/"
        },
        {
            "keys": [ "Q" ],
            "command": "server_"request"",
            "request": {
                "api": "/server/quit"
            }
        },
        {
            "keys": [ " " ],
            "command": "server_"request"",
            "request": {
                "api": "/player/toggle/play"
            }
        },
        {
            "keys": [ "0" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/volume/increase",
                "amount": 1
            }
        },
        {
            "keys": [ "9" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/volume/decrease"
                "amount": 1
            }
        },
        {
            "keys": [ "S" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/toggle/shuffle"
            }
        },
        {
            "keys": [ "R" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/toggle/repeat"
            }
        },
        {
            "keys": [ "N" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/toggle/next"
            }
        },
        {
            "keys": [ "n" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/play/next"
            }
        },
        {
            "keys": [ "p" ],
            "command": "server_"request"",
            "request": {
                "api": "/player/play/previous"
            }
        },
        {
            "keys": [ "a" ],
            "command": "server_"request"",
            "request": {
                "api": "/playlist/append"
            }
        },
        {
            "keys": [ "d" ],
            "command": "server_"request"",
            "request": {
                "api": "/playlist/remove"
            }
        },
        {
            "keys": [ "C" ],
            "command": "server_"request"",
            "request": {
                "api": "/playlist/clear"
            }
        },
        {
            "keys": [ "w" ],
            "command": "server_"request"",
            "request": {
                "api": "/playlist/move_up"
            }
        },
        {
            "keys": [ "s" ],
            "command": "server_"request"",
            "request": {
                "api": "/playlist/move_down"
            }
        },
    ]
}
